   "source": [
    "def my_decorator(func):\n",
    "    def run(name):\n",
    "        print(\"before executing the function\")\n",
    "        func(name)\n",
    "        print(\"after executing the function\")\n",
    "        \n",
    "    return run\n",
    "\n",
    "\n",
    "\n",
    "@my_decorator\n",
    "def main_fun(name):\n",
    "   print(name)\n",
    "\n",
    "\n",
    "\n",
    "main_fun(\"hello world\")"
   ]
  },
